#
# eta filters for semi-leptonic(mu)
#
module filterSemiMuonEta = EtaEventFilter{
  VInputTag input = {selectedLayer1Muons}
  PSet cuts = { using SemiMuonEtaCut }
  InputTag weight = eventWeight
}

module filterSemiJetsEta = EtaEventFilter{
  VInputTag input = {selectedLayer1Jets}
  PSet cuts = { using SemiJetsEtaCut }
  InputTag weight = eventWeight
}

#
# eta filters dileptonic(mu/mu)
#
module filterDiMuonEta = EtaEventFilter{
  VInputTag input = {selectedLayer1Muons}
  PSet cuts = { using DiMuonEtaCut }
  InputTag weight = eventWeight
}

module filterDiJetsEta = EtaEventFilter{
  VInputTag input = {selectedLayer1Jets}
  PSet cuts = { using DiJetsEtaCut }
  InputTag weight = eventWeight
}
