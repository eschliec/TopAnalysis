module filterMuonEta = EtaEventFilter{
  VInputTag input = {selectedLayer1Muons}
  PSet cuts = { using MuonEtaCut }
}

module filterElecEta = EtaEventFilter{
  VInputTag input = {selectedLayer1Elecs}
  PSet cuts = { using ElecEtaCut }
}

module filterJetsEta = EtaEventFilter{
  VInputTag input = {selectedLayer1Jets}
  PSet cuts = { using JetsEtaCut }
}
